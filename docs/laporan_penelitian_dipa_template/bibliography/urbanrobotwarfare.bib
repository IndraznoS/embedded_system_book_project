@inproceedings{Pradeep2023,
   abstract = {The paper focuses on the use of ESP32 as an Internet of Things (IoT) platform for affordable education. With the increasing importance of IoT in various industries, it is imperative to incorporate IoT in the education sector to equip students with the necessary skills to succeed in the digital age. The ESP32 platform has gained popularity due to its cost-effectiveness and versatility in implementing IoT projects. This research study proposes a novel methodology to implement ESP32 for educational purposes, collect and analyze data, and evaluate the effectiveness of this platform. The results suggest ESP32 as a viable and effective platform for affordable education, with advanced capabilities than other IoT platforms. This study contributes to the research literature on IoT and education, providing insights into the use of ESP32 for educational purposes.},
   author = {Aneesh Pradeep},
   doi = {10.1109/ICIRCA57980.2023.10220594},
